{
    "name": "zaimea/helpers",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "The ZaimeaLabs Helpers package.",
    "keywords": [
        "helpers",
        "laravel",
        "zaimea",
        "zaimealabs"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^1.12.23",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
